Program.Sub.loaddtWOs_wo_SOs.Start
F.Intrinsic.Control.Try
V.Local.sError.Declare
v.Local.sSQL.Declare
v.Local.iCnt.Declare
v.Local.sRet.Declare

Gui.FrmOrderReview.lblStatus.Caption("Loading Work Orders without Sales Order....")
Gui.FrmOrderReview.lblStatus.Visible(True)
Gui.FrmOrderReview.progressBar1.Value(0)
Gui.FrmOrderReview.progressBar1.Visible(True)

f.Intrinsic.Control.If(v.DataTable.dtWOs_wo_SO.Exists,=,True)
	f.Data.Datatable.Close("dtWOs_wo_SO")
f.Intrinsic.Control.EndIf

f.Data.Datatable.CreateFromSQL("dtWOs_wo_SO","con","Select JOB, SUFFIX, RTRIM(PART) as PART, LOCATION,PRODUCT_LINE, RTRIM(DESCRIPTION) as DESCRIPTION,'' as CUSTOMER_NAME ,QTY_ORDER, QTY_ORDER - QTY_COMPLETED AS REM_QTY, DATE_DUE,RTRIM(CUSTOMER) as CUSTOMER,JOB+SUFFIX as JS FROM V_JOB_HEADER WHERE SALES_ORDER = '' AND SALES_ORDER_LINE = ''",true)
f.Data.Dictionary.CreateFromSQL("dCustName","con","Select RTRIM(CUSTOMER), RTRIM(NAME_CUSTOMER) from V_CUSTOMER_MASTER")
f.Data.Dictionary.SetDefaultReturn("dCustName","")
f.Data.Datatable.FillFromDictionary("dtWOs_wo_SO","dCustName","Customer","CUSTOMER_NAME")
f.Data.Dictionary.Close("dCustName")

'add PO Lines
f.Data.Datatable.AddColumn("dtWOs_wo_SO","PO","String")
f.Data.Datatable.AddColumn("dtWOs_wo_SO","PO_Line","String")
f.Data.Datatable.CreateFromSQL("dtPOs","con","Select JOB+SUFFIX as JS, PURCHASE_ORDER,LEFT(RECORD_NO,3) as PO_LINE from V_PO_LINES")
f.Data.Dictionary.createFromDatatable("dPO","dtPOs","JS","PURCHASE_ORDER")
f.Data.Dictionary.createFromDatatable("dPOL","dtPOs","JS","PO_LINE")

f.Data.Dictionary.SetDefaultReturn("dPO","")
f.Data.Datatable.FillFromDictionary("dtWOs_wo_SO","dPO","JS","PO")
f.Data.Dictionary.Close("dPO")

f.Data.Dictionary.SetDefaultReturn("dPOL","")
f.Data.Datatable.FillFromDictionary("dtWOs_wo_SO","dPOL","JS","PO_Line")
f.Data.Dictionary.Close("dPOL")
'rem oper
f.Data.Datatable.AddColumn("dtWOs_wo_SO","Rem_OPS","String")


f.Intrinsic.Control.For(v.Local.iCnt,0,v.DataTable.dtWOs_wo_SO.RowCount--,1)
	Gui.FrmOrderReview.progressBar1.Value(v.Local.iCnt)
	f.Intrinsic.String.Build("Select RTRIM(DESCRIPTION) from V_JOB_OPERATIONS where DATE_COMPLETED = '1900-01-01' and JOB = '{0}' and SUFFIX = '{1}' and PART <> '' and DESCRIPTION not like '%SET UP%' and SEQ < 990000 order by SEQ",v.DataTable.dtWOs_wo_SO(v.Local.iCnt).JOB!FieldValTrim,v.DataTable.dtWOs_wo_SO(v.Local.iCnt).SUFFIX!FieldValTrim,v.Local.sSQL)
	f.ODBC.Connection!con.ExecuteAndReturn(v.Local.sSQL,v.Local.sRet)
	f.Intrinsic.Control.If(v.Ambient.executeAndReturnEOF,=,False)
		f.Intrinsic.String.Replace(v.Local.sRet,"#$#",", ",v.Local.sRet)
		f.Data.Datatable.SetValue("dtWOs_wo_SO",v.Local.iCnt,"Rem_OPS",v.Local.sRet)
	f.Intrinsic.Control.EndIf
f.Intrinsic.Control.Next(v.Local.iCnt)

'add WO additional info from custom table
f.Data.Datatable.AddColumn("dtWOs_wo_SO","PART_STATUS","String")
f.Data.Datatable.AddColumn("dtWOs_wo_SO","PART_LINKS","Boolean")
f.Data.Datatable.AddColumn("dtWOs_wo_SO","DATASHEETS","Boolean")
f.Data.Datatable.AddColumn("dtWOs_wo_SO","MAT_REQ","Boolean")
f.Data.Datatable.AddColumn("dtWOs_wo_SO","FINAL_ROUTER","Boolean")
f.Data.Datatable.AddColumn("dtWOs_wo_SO","ROUTER_COMPLETE","Boolean")
f.Data.Datatable.AddColumn("dtWOs_wo_SO","WO_PKG","Boolean")

f.Data.Datatable.FillFromDictionary("dtWOs_wo_SO","dPartStatus","JS","PART_STATUS")
f.Data.Dictionary.Close("dPartStatus")

f.Data.Datatable.FillFromDictionary("dtWOs_wo_SO","dPartLinks","JS","PART_LINKS")
f.Data.Dictionary.Close("dPartLinks")

f.Data.Datatable.FillFromDictionary("dtWOs_wo_SO","dDATASHEETS","JS","DATASHEETS")
f.Data.Dictionary.Close("dDATASHEETS")

f.Data.Datatable.FillFromDictionary("dtWOs_wo_SO","dMAT_REQ","JS","MAT_REQ")
f.Data.Dictionary.Close("dMAT_REQ")

f.Data.Datatable.FillFromDictionary("dtWOs_wo_SO","dFINAL_ROUTER","JS","FINAL_ROUTER")
f.Data.Dictionary.Close("dFINAL_ROUTER")

f.Data.Datatable.FillFromDictionary("dtWOs_wo_SO","dROUTER_COMPLETE","JS","ROUTER_COMPLETE")
f.Data.Dictionary.Close("dROUTER_COMPLETE")

f.Data.Datatable.FillFromDictionary("dtWOs_wo_SO","dWO_PKG","JS","WO_PKG")
f.Data.Dictionary.Close("dWO_PKG")

f.Data.Datatable.RemoveColumn("dtWOs_wo_SO","JS")
F.Intrinsic.Control.CallSub(LoadGVWOs_wo_SO)

F.Intrinsic.Control.Catch
	F.Intrinsic.String.Build("Project: {0}{1}{1}Subroutine: {2}{1}Error Occurred {3} with description {4}{1}{1}GAB Version: {5}",V.Ambient.ScriptPath,V.Ambient.Newline,V.Ambient.CurrentSubroutine,V.Ambient.ErrorNumber,V.Ambient.ErrorDescription,V.Ambient.GABVersion,V.Local.sError)
	F.Intrinsic.UI.Msgbox(V.Local.sError)
	F.Intrinsic.Control.End 
F.Intrinsic.Control.EndTry
Program.Sub.loaddtWOs_wo_SOs.End
